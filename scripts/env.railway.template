# Railway Environment Variables Template
# Copy this file to .env.railway.local and fill in your actual values
# DO NOT commit the .local version to git

# API Keys - Get these from your AI service providers
TRIPO_API_KEY=your_tripo_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
STABILITY_API_KEY=your_stability_api_key_here
RECRAFT_API_KEY=your_recraft_api_key_here
REPLICATE_API_KEY=your_replicate_api_key_here
FLUX_API_KEY=your_flux_api_key_here

# Supabase Configuration - Get from your Supabase project settings
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Application Configuration
BFF_BASE_URL=https://your-railway-domain.railway.app

# GitHub Repository (for Railway deployment)
GITHUB_REPO=your-username/your-repo-name

# Redis URL (automatically set by Railway)
REDIS_URL=${{Redis.REDIS_URL}} 